{
  "name": "mindy/orm-fields",
  "type": "library",
  "description": "Mindy Orm Extra Fields Collection",
  "license": "MIT",
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mindy\\Orm\\Fields\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mindy\\Orm\\Fields\\Tests\\": "./Tests/"
    }
  },
  "require": {
    "php": ">=7.0",
    "mindy/orm": ">=4.0",
    "symfony/validator": "~3.0|~4.0"
  },
  "require-dev": {
    "symfony/http-foundation": "~3.0|~4.0",
    "symfony/routing": "~3.0|~4.0",
    "phpunit/phpunit": "^6.5"
  }
}
